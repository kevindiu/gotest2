{{template "header.tmpl" .}}

{{range .Funcs}}
    {{if or .Render (ne .ExistingSource "")}}
        {{if .TypeParams}}
            {{template "generic_test" .}}
        {{else}}
            {{template "standard_test" .}}
        {{end}}
    {{end}}
{{end}}

{{range .Funcs}}
    {{if or .Render (ne .ExistingSource "")}}
        {{if .TypeParams}}
            {{template "generic_runner" .}}
        {{end}}
    {{end}}
{{end}}

{{range .Funcs}}
    {{if or .Render (ne .ExistingSource "")}}
        {{if .TypeParams}}
            {{template "generic_struct" .}}
        {{else}}
            {{if not .HasLocalStruct}}
                {{template "standard_struct" .}}
            {{end}}
        {{end}}
    {{end}}
{{end}}
