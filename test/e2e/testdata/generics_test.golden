func testMax[T int | float64](t *testing.T, cases []testCaseMax[T]) {
	t.Parallel()
	for _, tt := range cases {
		defaultValidate := func(t *testing.T, got0 T, tt *testCaseMax[T]) error {
			if !reflect.DeepEqual(got0, tt.want.want0) {
				return fmt.Errorf("Max() got0 = %v, want %v", got0, tt.want.want0)
			}
			return nil
		}
		defaultInit := func(t *testing.T, tt *testCaseMax[T]) {}
		defaultCleanup := func(t *testing.T, tt *testCaseMax[T]) {}
		t.Run(tt.name, func(t *testing.T) {
			t.Parallel()
			if tt.init == nil {
				tt.init = defaultInit
			}
			tt.init(t, &tt)
			if tt.cleanup == nil {
				tt.cleanup = defaultCleanup
			}
			defer tt.cleanup(t, &tt)
			got0 := Max[T](
				tt.args.a,
				tt.args.b,
			)
			if tt.validate == nil {
				tt.validate = defaultValidate
			}
			if err := tt.validate(t, got0, &tt); err != nil {
				t.Errorf("Max() validation failed: %v", err)
			}
		})
	}
}

